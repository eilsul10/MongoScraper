<nav>
  <div class="nav-wrapper">
    <a href="#" class="brand-logo">HuffPost Scraper</a>
    <ul id="nav-mobile" class="right hide-on-med-and-down">
      <li><a href="/scrape">Start Scraping</a></li>
      <li><a href="/saved" id="viewSaved">Saved Articles</a></li>
    </ul>
  </div>
</nav>

<div class="container"><center>Click on the title to add a note! Click on the 'X' to delete.</center></div>
{{#each hbsObject}}
{{#unless saved}}
{{#each this}}
<div class="row">
  <div class="col s12 teal lighten-2">
    <a class="waves-effect waves-light btn-small" id="deletebtn" data-id="{{_id}}">X</a>
<h3>{{title}}</h3>
<h5>{{link}}</h5>
<a class="waves-effect waves-light btn-small" id="saveArticle" data-id="{{_id}}">Save Article</a>
<br><br>
</div>
</div>
{{/each}}
{{/unless}}
{{/each}}

<!-- Modal Structure -->
<div id="modal1" class="modal">
  <div class="modal-content">
    <div class="input-field col s12">
      <textarea id="textarea1" class="materialize-textarea"></textarea>
      <label for="textarea1">Notes</label>
    </div>
  </div>
  <div class="modal-footer">
    <a href="#!" class="modal-close waves-effect waves-green btn-flat">Done</a>
  </div>
</div>

