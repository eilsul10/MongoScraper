<nav>
  <div class="nav-wrapper">
    <a href="#" class="brand-logo">HuffPost Scraper</a>
    <ul id="nav-mobile" class="right hide-on-med-and-down">
      <li><a href="/">Home</a></li>
    </ul>
  </div>
</nav>



  <div class="row">
    <div class="col s12 teal lighten-2">
        <table id= "populate"></table>
      </div>
  </div>

  {{!-- <a class="waves-effect waves-light btn modal-trigger" data-target="modal1" href="#modal1">Modal</a> --}}

  <!-- Modal Structure -->
  <div id="modal1" class="modal">
    <div class="modal-content">
      <div class="input-field col s12">
          <textarea id="textarea1" class="materialize-textarea"></textarea>
          <label for="textarea1">Notes</label>
        </div>
    </div>
    <div class="modal-footer">
      <a href="#!" class="modal-close waves-effect waves-green btn-flat">Done</a>
    </div>
  </div>

  {{#each articles}}
    {{#if saved}}
      <figure class="storyPanel clearfix">
        <a href="{{storyUrl}}" class="pull-right" target="_blank">
          <div class="imgBox">
            <img src="{{imgUrl}}" alt="" class="storyImg" height="150">
          </div>
        </a>
        {{#if storyUrl}}
          <a href="{{storyUrl}}" class="storyLink" target="_blank">
            <h2 class="storyHeadline">{{headline}}</h2>
          </a>
        {{else}}
          <h3 class="storyHeadline">{{headline}}</h3>
        {{/if}}
        <h5 class="storyByline">{{byLine}}</h5>
        <h6 class="storySummary">{{summary}}</h6>
        <button class="addNote storyButton" data-id="{{_id}}">Add Note</button>
        <button class="deleteArticle storyButton" data-id="{{_id}}">Delete Article</button>
      </figure>
    {{/if}}
  {{/each}}